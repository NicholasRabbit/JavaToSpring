<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">


<servlet>
	<servlet-name>a</servlet-name>
	<servlet-class>com.javaweb.request.HttpServletRequest_A</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>a</servlet-name>
	<url-pattern>/user/request_a</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>b</servlet-name>
	<servlet-class>com.javaweb.request.HttpServletRequest_B</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>b</servlet-name>
	<url-pattern>/user/request_b</url-pattern>
</servlet-mapping>

</web-app>
